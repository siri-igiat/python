    "def isprime(n):\n",
    "    status=0\n",
    "    for i in range(2,n//2):\n",
    "        if n%i==0:\n",
    "            status=1\n",
    "    if status==0:\n",
    "         print(\"n is prime\")\n",
    "    else:\n",
    "        print(\"n is not prime\")\n",
    "n=int(input(\"enter a number\"))\n",
    "isprime(n)"
   ]
  }
